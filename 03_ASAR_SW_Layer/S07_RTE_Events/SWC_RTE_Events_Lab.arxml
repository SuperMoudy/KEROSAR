<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 file//../../AUTOSAR_ARXML_Schema/AUTOSAR_00051.xsd">
	<AR-PACKAGES>
		<AR-PACKAGE>
			<!-- Learn in depth, Eng. Mahmoud Hamdy, Unit3: SW Layer, Session7: RTE Events -->
			<SHORT-NAME>RTE_Events_Lab</SHORT-NAME>
			<ELEMENTS>
				<!-- Interfaces -->
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRI_Output</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>SumResult</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRI_Input</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Val1</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Val2</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<!-- SWCs -->
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>SWC1</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>SR_PP_Sum</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/RTE_Events_Lab/SRI_Output</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>SR_RP_Sum</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/RTE_Events_Lab/SRI_Input</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
					</PORTS>
					<INTERNAL-BEHAVIORS>
						<SWC-INTERNAL-BEHAVIOR>
							<SHORT-NAME>SWC1_Behavior</SHORT-NAME>
							<EXCLUSIVE-AREAS>
								<EXCLUSIVE-AREA>
									<SHORT-NAME>EXCArea1</SHORT-NAME>
								</EXCLUSIVE-AREA>
							</EXCLUSIVE-AREAS>
							<EVENTS>
								<TIMING-EVENT>
									<SHORT-NAME>TE_10ms</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/RTE_Events_Lab/SWC1/SWC1_Behavior/SUM_Runnable</START-ON-EVENT-REF>
									<PERIOD>0.01</PERIOD>
								</TIMING-EVENT>
								<INIT-EVENT>
									<SHORT-NAME>InitEvent</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/RTE_Events_Lab/SWC1/SWC1_Behavior/SUM_Runnable</START-ON-EVENT-REF>
								</INIT-EVENT>
								<DATA-RECEIVED-EVENT>
									<SHORT-NAME>DRCEvent</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/RTE_Events_Lab/SWC1/SWC1_Behavior/SUM_Runnable</START-ON-EVENT-REF>
									<DATA-IREF>
										<TARGET-DATA-ELEMENT-REF DEST="VARIABLE-DATA-PROTOTYPE">/RTE_Events_Lab/SRI_Input/Val1</TARGET-DATA-ELEMENT-REF>
									</DATA-IREF>
								</DATA-RECEIVED-EVENT>
								<DATA-WRITE-COMPLETED-EVENT>
									<SHORT-NAME>DWCEvent</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/RTE_Events_Lab/SWC1/SWC1_Behavior/SUM_Runnable</START-ON-EVENT-REF>
									<EVENT-SOURCE-REF DEST="VARIABLE-ACCESS">/RTE_Events_Lab/SWC1/SWC1_Behavior/SUM_Runnable/SumResult_Data</EVENT-SOURCE-REF>
								</DATA-WRITE-COMPLETED-EVENT>
							</EVENTS>
							<RUNNABLES>
								<RUNNABLE-ENTITY>
									<SHORT-NAME>SUM_Runnable</SHORT-NAME>
									<MINIMUM-START-INTERVAL>0.0</MINIMUM-START-INTERVAL>
									<CAN-BE-INVOKED-CONCURRENTLY>false</CAN-BE-INVOKED-CONCURRENTLY>
									<DATA-READ-ACCESSS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>Val1_Data</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF DEST="R-PORT-PROTOTYPE">/RTE_Events_Lab/SWC1/SR_RP_Sum</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/RTE_Events_Lab/SRI_Input/Val1</TARGET-DATA-PROTOTYPE-REF>
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>Val2_Data</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF DEST="R-PORT-PROTOTYPE">/RTE_Events_Lab/SWC1/SR_RP_Sum</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/RTE_Events_Lab/SRI_Input/Val2</TARGET-DATA-PROTOTYPE-REF>
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
									</DATA-READ-ACCESSS>
									<DATA-WRITE-ACCESSS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>SumResult_Data</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF DEST="P-PORT-PROTOTYPE">/RTE_Events_Lab/SWC1/SR_PP_Sum</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/RTE_Events_Lab/SRI_Output/SumResult</TARGET-DATA-PROTOTYPE-REF>
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
									</DATA-WRITE-ACCESSS>
									<SYMBOL>SUM</SYMBOL>
								</RUNNABLE-ENTITY>
							</RUNNABLES>
						</SWC-INTERNAL-BEHAVIOR>
					</INTERNAL-BEHAVIORS>
				</APPLICATION-SW-COMPONENT-TYPE>
			</ELEMENTS>
		</AR-PACKAGE>
	</AR-PACKAGES>
</AUTOSAR>